{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-xml-transformation-dev"
		},
		"ls_sql_on_prem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_on_prem'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sapdadlsdev.dfs.core.windows.net/"
		},
		"ls_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvpddev.vault.azure.net/"
		},
		"ls_sql_on_prem_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pawan"
		},
		"ls_sqldb_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLConnectionString"
		},
		"pe_adls_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/fae6a450-5060-4ef3-96d1-f4ca2fab4cef/resourceGroups/rg-pd-dev/providers/Microsoft.Storage/storageAccounts/sapdadlsdev"
		},
		"pe_adls_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"pe_sql_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/fae6a450-5060-4ef3-96d1-f4ca2fab4cef/resourceGroups/rg-pd-dev/providers/Microsoft.Sql/servers/sqlpd"
		},
		"pe_sql_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-xml-transformation-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sql_on_prem_connectionString": {
						"value": "[parameters('ls_sql_on_prem_connectionString')]"
					},
					"ls_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_sql_on_prem_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_on_prem_properties_typeProperties_userName')]"
					},
					"pe_adls_properties_privateLinkResourceId": {
						"value": "[parameters('pe_adls_properties_privateLinkResourceId')]"
					},
					"pe_adls_properties_groupId": {
						"value": "[parameters('pe_adls_properties_groupId')]"
					},
					"pe_sql_properties_privateLinkResourceId": {
						"value": "[parameters('pe_sql_properties_privateLinkResourceId')]"
					},
					"pe_sql_properties_groupId": {
						"value": "[parameters('pe_sql_properties_groupId')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_sqldb_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_sqldb_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}